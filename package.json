{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "engines": {
    "node": ">=8 <10"
  },
  "scripts": {
    "start": "tsc && node app_bin/server",
    "start:d": "node -r ts-node/register --inspect app/server",
    "dev:d": "node-dev --no-deps -r ts-node/register --inspect app/server.ts",
    "dev": "node-dev --no-deps -r ts-node/register app/server.ts",
    "test": "npm run eslint && npm run tscheck && npm run test:r",
    "t": "jest",
    "test:w": "jest --watch",
    "test:r": "jest --runInBand --verbose",
    "test:cov": "npm run test:r -- --collectCoverage",
    "test:d": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand --testMatch \"<rootDir>/app/**/*.spec.ts\"",
    "tscheck": "echo tscheck... && tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "tsclint": "tsc --noEmit --pretty false --strictNullChecks --forceConsistentCasingInFileNames --noImplicitReturns --noImplicitThis --noUnusedLocals",
    "tsclint:w": "npm run tsclint -- --watch",
    "eslint": "gulp eslint",
    "eslint:fix": "eslint app --ext \"ts\" --fix",
    "eslint:w": "gulp eslint:w",
    "lint:w": "concurrently -- \"npm run tsclint:w\" \"npm run eslint:w\"",
    "test:m": "stryker run",
    "dev:update": "updtr --out --save exact --to latest --test \"npm test && npm run test:m\""
  },
  "dependencies": {
    "@nestjs/common": "4.6.6",
    "@nestjs/core": "4.6.6",
    "@nestjs/graphql": "2.0.0",
    "@nestjs/microservices": "4.6.6",
    "@nestjs/swagger": "1.1.4",
    "@nestjs/testing": "4.6.6",
    "@nestjs/typeorm": "2.0.0",
    "@nestjs/websockets": "4.6.6",
    "@types/convict": "4.1.1",
    "apollo-server-express": "1.3.4",
    "body-parser": "1.18.2",
    "class-transformer": "0.1.9",
    "class-validator": "0.8.5",
    "convict": "4.2.0",
    "express-jwt": "5.3.1",
    "graphql": "0.13.2",
    "graphql-tools": "2.23.1",
    "mongodb": "3.0.5",
    "redis": "2.8.0",
    "reflect-metadata": "0.1.12",
    "rxjs": "5.5.8",
    "sqlite3": "4.0.0",
    "ts-node": "5.0.1",
    "tslib": "1.9.0",
    "typescript": "2.7.2",
    "typeorm": "0.2.0-alpha.42"
  },
  "devDependencies": {
    "@types/express": "4.11.1",
    "@types/graphql": "0.12.7",
    "@types/gulp": "4.0.5",
    "@types/jest": "22.2.2",
    "@types/node": "9.6.1",
    "@types/supertest": "2.0.4",
    "@types/yargs": "11.0.0",
    "concurrently": "3.5.1",
    "eslint": "4.19.1",
    "eslint-import-resolver-node": "0.3.2",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-nestjs": "1.2.2",
    "eslint-plugin-tslint": "2.1.0",
    "eslint-plugin-typescript": "0.11.0",
    "eslint-plugin-unicorn": "4.0.2",
    "gulp": "4.0.0",
    "gulp-eslint": "4.0.2",
    "gulp-if": "2.0.2",
    "gulp-ignore": "2.0.2",
    "gulp-load-plugins": "1.5.0",
    "gulp-memory-cache": "0.3.0",
    "jest": "22.4.3",
    "jest-cli": "22.4.3",
    "jest-runtime": "22.4.3",
    "loud-rejection": "1.6.0",
    "node-dev": "3.1.3",
    "stryker": "0.20.1",
    "stryker-api": "0.14.0",
    "stryker-html-reporter": "0.12.4",
    "stryker-jest-runner": "0.5.1",
    "stryker-typescript": "0.9.4",
    "supertest": "3.0.0",
    "ts-jest": "22.4.2",
    "tslint": "5.9.1",
    "typescript-eslint-parser": "14.0.0",
    "updtr": "2.0.0"
  }
}
