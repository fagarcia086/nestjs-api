{
  "name": "nest-typescript-starter",
  "version": "0.0.0-dev",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "engines": {
    "node": ">=8.9 <11"
  },
  "scripts": {
    "start": "tsc && npm run app:bin",
    "dev:n:inspect": "node-dev --no-deps -r ts-node/register/transpile-only --inspect src/server.ts --program=tsnode",
    "dev:n": "ts-node-dev --no-deps --transpileOnly src/server.ts --program=tsnode",
    "dev": "npm run dev:w",
    "dev:w": "run-p \"webpack -- --watch {@}\" \"app:bin -- --program=webpack\"",
    "dev:w:inspect": "run-p \"webpack -- --watch {@}\" \"app:bin:inspect -- --program=webpack\" --",
    "predev:w": "rimraf app_bin",
    "webpack": "webpack",
    "app:bin": "just-wait -p \"app_bin/server.js\" && node app_bin/server.js",
    "app:bin:inspect": "just-wait -p \"app_bin/server.js\" && node --inspect app_bin/server.js",
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "t": "jest",
    "test:w": "jest --watch",
    "test:r": "jest --runInBand --verbose",
    "test:cov": "npm run test:r -- --collectCoverage",
    "test:d": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand --testMatch \"<rootDir>/src/**/*.spec.ts\"",
    "tscheck": "echo tscheck... && tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "tsclint": "tsc --noEmit --strictNullChecks --forceConsistentCasingInFileNames --noImplicitReturns --noImplicitThis --noUnusedLocals",
    "tsclint:w": "npm run tsclint -- --watch",
    "eslint": "gulp eslint",
    "eslint:fix": "eslint app --ext \"ts\" --fix",
    "eslint:w": "gulp eslint:w",
    "lint:w": "run-p tsclint:w eslint:w",
    "test:m": "stryker run",
    "dev:update": "updtr --out --save exact --to latest --test \"npm test && npm run test:m\""
  },
  "dependencies": {
    "@nestjs/common": "5.2.2",
    "@nestjs/core": "5.2.2",
    "@nestjs/graphql": "3.0.0",
    "@nestjs/jwt": "0.1.3",
    "@nestjs/microservices": "5.2.2",
    "@nestjs/swagger": "2.4.6",
    "@nestjs/testing": "5.2.2",
    "@nestjs/typeorm": "5.2.0",
    "@nestjs/websockets": "5.2.2",
    "@types/convict": "4.2.0",
    "apollo-server-express": "1.4.0",
    "body-parser": "1.18.3",
    "class-transformer": "0.1.9",
    "class-validator": "0.9.1",
    "convict": "4.3.2",
    "dataloader": "1.4.0",
    "graphql": "0.13.2",
    "graphql-tools": "3.0.5",
    "loud-rejection": "1.6.0",
    "mongodb": "3.1.4",
    "redis": "2.8.0",
    "reflect-metadata": "0.1.12",
    "rxjs": "6.2.2",
    "rxjs-compat": "6.2.2",
    "sqlite3": "4.0.2",
    "ts-node": "7.0.1",
    "tslib": "1.9.3",
    "tslint-sonarts": "^1.7.0",
    "typeorm": "0.2.7",
    "typescript": "2.9.2"
  },
  "devDependencies": {
    "@types/express": "4.16.0",
    "@types/graphql": "0.13.4",
    "@types/gulp": "4.0.5",
    "@types/jest": "23.3.1",
    "@types/lodash": "4.14.116",
    "@types/node": "10.9.2",
    "@types/rimraf": "2.0.2",
    "@types/supertest": "2.0.5",
    "@types/webpack": "4.4.11",
    "@types/webpack-env": "1.13.6",
    "@types/yargs": "11.1.1",
    "eslint": "5.4.0",
    "eslint-import-resolver-node": "0.3.2",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "21.22.0",
    "eslint-plugin-nestjs": "1.2.3",
    "eslint-plugin-tslint2": "2.2.4",
    "eslint-plugin-typescript": "0.12.0",
    "eslint-plugin-unicorn": "6.0.0",
    "gulp": "4.0.0",
    "gulp-debug": "4.0.0",
    "gulp-eslint": "5.0.0",
    "gulp-if": "2.0.2",
    "gulp-ignore": "2.0.2",
    "gulp-load-plugins": "1.5.0",
    "gulp-memory-cache": "0.3.0",
    "jest": "23.5.0",
    "jest-cli": "23.5.0",
    "jest-runtime": "23.5.0",
    "json-socket": "0.3.0",
    "just-wait": "1.0.11",
    "node-dev": "3.1.3",
    "npm-run-all": "4.1.3",
    "rimraf": "2.6.2",
    "source-map-support": "0.5.9",
    "stryker": "0.29.0",
    "stryker-api": "0.21.0",
    "stryker-html-reporter": "0.16.1",
    "stryker-jest-runner": "1.1.1",
    "stryker-typescript": "0.13.1",
    "supertest": "3.1.0",
    "ts-jest": "23.1.4",
    "ts-loader": "4.5.0",
    "ts-node-dev": "1.0.0-pre.26",
    "tslint": "5.11.0",
    "tslint-clean-code": "0.2.7",
    "tslint-microsoft-contrib": "5.2.1",
    "typescript-eslint-parser": "17.0.1",
    "updtr": "2.0.0",
    "webpack": "4.17.1",
    "webpack-cli": "3.1.0",
    "webpack-loader-helper": "1.0.0",
    "webpack-node-externals": "1.7.2"
  }
}
